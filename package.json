{
  "name": "newsrater",
  "version": "1.0.0",
  "description": "API service to quickly crawl and retrieve data from the web",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "cleanNews": "babel-node scheduler/updatePublications-cleanNews.js",
    "createNew": "babel-node inquirer/createNewPublication.js",
    "createPublications": "babel-node scheduler/createPublications-readFromFile.js",
    "calculateOverallRating": "babel-node scheduler/updatePublications-calculateOverallRating.js",
    "getAlexa": "babel-node scheduler/updatePublications-getAlexa.js",
    "getGoogleNews": "babel-node scheduler/updatePublications-getGoogleNews.js",
    "getIPSO": "babel-node scheduler/updatePublications-getIndependentPressStandardsOrganisation.js",
    "getNews": "npm-run-all --sequential getGoogleNews getNewsApi cleanNews",
    "getNewsApi": "babel-node scheduler/updatePublications-getNewsApi.js",
    "getPCC": "babel-node scheduler/updatePublications-getPressComplaintsCommission.js",
    "getPressComplaints": "npm-run-all --sequential getPCC getIPSO",
    "getPrices": "babel-node scheduler/updatePublications-getPrices.js",
    "getUkCirculation": "babel-node scheduler/updatePublications-getUkCirculation.js",
    "getWikipedia": "babel-node scheduler/updatePublications-getWikipedia.js",
    "welcome": "node welcome.js",
    "scheduler": "npm-run-all --sequential welcome getAlexa getNews calculateOverallRating",
    "lint": "eslint -c .eslintrc ./",
    "precommit": "npm run lint",
    "test": "echo No tests to run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namroodinc/newsrater.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/namroodinc/newsrater/issues"
  },
  "homepage": "https://github.com/namroodinc/newsrater#readme",
  "dependencies": {
    "asciify": "^1.3.5",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-node6": "^11.0.0",
    "contentful": "^5.0.5",
    "contentful-management": "^4.1.2",
    "d3": "^4.12.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "inquirer": "^4.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.0",
    "npm-run-all": "^4.1.2",
    "osmosis": "^1.1.4",
    "parse-domain": "^2.0.0",
    "read-file": "^0.2.0",
    "sentiment": "^4.1.0",
    "superagent": "^3.8.2",
    "table-scraper": "^0.1.0",
    "twitter": "^1.7.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.11.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3"
  }
}
